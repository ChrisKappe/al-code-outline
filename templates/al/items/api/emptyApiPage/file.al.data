page Id "$itemname$"
{
    PageType = API;
    Caption = 'apiPageName';
    APIPublisher = 'PublisherName';
    APIGroup = 'GroupName';
    APIVersion = 'VersionList';
    EntityName = 'EntityName';
    EntitySetName = 'EntitySetName';
    SourceTable = TableName;
    DelayedInsert = true;
    
    layout
    {
        area(Content)
        {
            repeater(GroupName)
            {
                field(fieldName; NameSource)
                {
                    Caption = 'fieldCaption';
                    
                }
            }
        }
    }
}